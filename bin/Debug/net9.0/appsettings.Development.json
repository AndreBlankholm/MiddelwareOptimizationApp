{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5294"
      }
    },
    "Limits": {
      "MaxConcurrentConnections": 50,
      "MaxConcurrentUpgradedConnections": 50,
      "MaxRequestBodySize": 50000000,
      "RequestHeadersTimeout": "00:01:00",
      "KeepAliveTimeout": "00:05:00"
    }
  }
}
